<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts  
Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="agent" extends="struts-default">
		<action name="agentlogin" class="com.flopper.framework.common.agentloginbean">
			<result name="input">agentlogin.jsp</result>
			<result name="error">agentlogin.jsp</result>
			<result name="agent">agentlanding.jsp</result>
			<result name="admin">adminlanding.jsp</result>
		</action>
		
		
		<action name="agentlanding" class="com.flopper.framework.common.agentlandingbean">
			<result name="logout">agentlogin.jsp</result>
			

		</action>
	</package>



</struts> 