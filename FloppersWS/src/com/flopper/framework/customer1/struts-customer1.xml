<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts  
Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="customer1" extends="struts-default">


		<action name="custlogin1" class="com.flopper.framework.customer1.custloginbean">
			<result name="input">custlogin.jsp</result>
			<result name="error">custlogin.jsp</result>
			<result name="success">custlanding.jsp</result>
		</action>

		<action name="custlanding1" class="com.flopper.framework.customer1.custlandingbean">
			<result name="logout">custlogin.jsp</result>
			<result name="appointment">custapointmentbooking.jsp</result>

		</action>
		<action name="custapointmentbooking1"
			class="com.flopper.framework.customer1.custapointmentbookingbean">
			<result name="input">custapointmentbooking.jsp</result>
			<result name="success">custsuccessmessage.jsp</result>
			<result name="error">custapointmentbooking.jsp</result>
		</action>




	</package>



</struts> 